{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Frontend Webpack Template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.build.conf.js",
    "test": "npm run stylelint && npm run eslint && npm run html-validate && npm run pug-lint",
    "stylelint": "npx stylelint \"./src/assets/scss/**/*.scss\" --config .stylelintrc.json --config-basedir ./ --fix",
    "eslint": "eslint ./src/js/**/*.js --cache --cache-location ./.cache/.eslintcache --fix",
    "html-validate": "html-validate ./dist/*.html",
    "pug-lint": "pug-lint --config .pug-lintrc.json src/pug/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "browserslist": [
    ">1%",
    "not ie > 0",
    "not op_mini > 0",
    "not dead"
  ],
  "keywords": [
    "webpack",
    "js",
    "scss",
    "pug",
    "svg"
  ],
  "author": "Andrii Helever <gelbman20@gmail.com>",
  "contributors": [
    "Kateryna Minakova <retoyfer@gmail.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^3.2.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-validate": "^3.2.0",
    "html-webpack-plugin": "^4.4.0",
    "husky": "^4.2.5",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.11.0",
    "modern-css-reset": "^1.1.1",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.0",
    "pug-lint": "^2.6.0",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg-spritemap-webpack-plugin": "^3.5.9",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "swiper": "^5.2.1",
    "vanilla-lazyload": "^17.1.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1"
  }
}
